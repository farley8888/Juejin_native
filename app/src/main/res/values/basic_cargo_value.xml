<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- ###################### styles #########################-->

    <!-- cargo base -->
    <style name="Cargo" />

    <!-- match parent -->
    <style name="JJ.MatchParent" parent="Cargo">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!-- match vertical -->
    <style name="JJ.MatchParent.Vertical" parent="JJ.MatchParent">
        <item name="android:orientation">vertical</item>
    </style>

    <!-- match horizontal -->
    <style name="JJ.MatchParent.Horizontal" parent="JJ.MatchParent">
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- wrap content -->
    <style name="JJ.WrapContent" parent="Cargo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- wrap vertical -->
    <style name="JJ.WrapContent.Vertical" parent="JJ.WrapContent">
        <item name="android:orientation">vertical</item>
    </style>

    <!-- wrap horizontal -->
    <style name="JJ.WrapContent.Horizontal" parent="JJ.WrapContent">
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- width match parent -->
    <style name="JJ.WidthFill" parent="Cargo">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!-- width match vertical -->
    <style name="JJ.WidthFill.Vertical" parent="JJ.WidthFill">
        <item name="android:orientation">vertical</item>
    </style>

    <!-- width match vertical background -->
    <style name="JJ.WidthFill.Vertical.WhiteBackground" parent="JJ.WidthFill.Vertical">
        <item name="android:background">@color/white</item>
    </style>

    <!-- width match horizontal -->
    <style name="JJ.WidthFill.Horizontal" parent="JJ.WidthFill">
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- width match horizontal background -->
    <style name="JJ.WidthFill.Horizontal.WhiteBackground" parent="JJ.WidthFill.Horizontal">
        <item name="android:background">@color/white</item>
    </style>

    <!-- width match horizontal background i -->
    <style name="JJ.WidthFill.Horizontal.WhiteBackground.Item" parent="JJ.WidthFill.Horizontal">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <!-- height match parent -->
    <style name="JJ.HeightFill" parent="Cargo">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <!-- height match vertical -->
    <style name="JJ.HeightFill.Vertical" parent="JJ.HeightFill">
        <item name="android:orientation">vertical</item>
    </style>

    <!-- height match horizontal -->
    <style name="JJ.HeightFill.Horizontal" parent="JJ.HeightFill">
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- match weight one -->
    <style name="JJ.MatchParent.Weight" parent="JJ.MatchParent">
        <item name="android:layout_weight">1</item>
    </style>

    <!-- width weight one -->
    <style name="JJ.WidthFill.Weight" parent="JJ.WidthFill">
        <item name="android:layout_weight">1</item>
    </style>

    <!-- height weight one -->
    <style name="JJ.HeightFill.Weight" parent="JJ.HeightFill">
        <item name="android:layout_weight">1</item>
    </style>

    <!-- line horizontal -->
    <style name="JJ.Line.Horizontal" parent="JJ.WidthFill">
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/colorDivider</item>
    </style>

    <!-- line horizontal margin -->
    <style name="JJ.Line.Horizontal.Margin" parent="JJ.Line.Horizontal">
        <item name="android:layout_marginLeft">15dp</item>
    </style>

    <!-- line vertical -->
    <style name="JJ.Line.Vertical" parent="JJ.HeightFill">
        <item name="android:layout_width">1px</item>
        <item name="android:background">@color/colorDivider</item>
    </style>

    <!-- label primary -->
    <style name="JJ.Label.Primary" parent="JJ.WrapContent">
